{
  "name": "Canvas_to_Google_Calendar",
  "version": "1.0.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/content_script.js"]
    }
  ],
  "icons": {
    "96": "calendar-icon-96.png"
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "Canvas_to_Google_Calendar",
    "default_popup": "/popup.html"
  },
  "permissions": ["storage"]
}
